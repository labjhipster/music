<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Album.
    -->
    <changeSet id="20201218072252-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="artist_id"
                                 baseTableName="album"
                                 constraintName="fk_album_artist_id"
                                 referencedColumnNames="id"
                                 referencedTableName="artist"/>

        <addForeignKeyConstraint baseColumnNames="genre_id"
                                 baseTableName="album"
                                 constraintName="fk_album_genre_id"
                                 referencedColumnNames="id"
                                 referencedTableName="genre"/>

    </changeSet>
</databaseChangeLog>
